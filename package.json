{
  "name": "dciscsstemplate",
  "version": "1.0.0",
  "description": "https://sass-lang.com/   https://sass-lang.com/guide   https://github.com/sass/sass-site",
  "main": "index.js",
  "scripts": {
    "start": "run-p watch watch:styles",
    "build": "run-s clean clean:styles build:styles copy",
    "deploy": "run-s build publish",
    "build:styles": "sass src/scss:src/styles",
    "watch": "live-server src",
    "watch:styles": "sass src/scss:src/styles --watch",
    "clean": "del-cli dist",
    "clean:styles": "del-cli src/styles",
    "copy": "mkdir dist && xcopy src dist /e /exclude:excludelist.txt",
    "publish": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelosperalta/dciScssTemplate.git"
  },
  "author": "Marcelo Soares Peralta",
  "license": "ISC",
  "devDependencies": {
    "del-cli": "^3.0.1",
    "gh-pages": "^2.1.1",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.23.0-module.beta.1"
  },
  "bugs": {
    "url": "https://github.com/marcelosperalta/dciScssTemplate/issues"
  },
  "homepage": "https://github.com/marcelosperalta/dciScssTemplate#readme",
  "dependencies": {
    "dot-prop": "^5.2.0"
  }
}
